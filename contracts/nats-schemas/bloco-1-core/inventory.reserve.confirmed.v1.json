{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.vertikon.com/bloco-1-core/inventory.reserve.confirmed.v1.json",
  "title": "inventory.reserve.confirmed.v1",
  "type": "object",
  "required": [
    "event_id",
    "reservation_id",
    "sku",
    "location",
    "quantity",
    "confirmed_at",
    "trace_id"
  ],
  "properties": {
    "event_id": {
      "type": "string",
      "format": "uuid"
    },
    "reservation_id": {
      "type": "string",
      "format": "uuid"
    },
    "sku": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "quantity": {
      "type": "integer",
      "minimum": 1
    },
    "batch_code": {
      "type": "string"
    },
    "expiry_date": {
      "type": "string",
      "format": "date"
    },
    "trace_id": {
      "type": "string"
    },
    "confirmed_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
